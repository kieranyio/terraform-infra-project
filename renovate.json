{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchDatasources": ["terraform-module"],
      "registryUrls": ["https://app.terraform.io"]
    }
  ],
  "hostRules": [
    {
      "matchHost": "app.terraform.io",
      "encrypted": {
        "token": ""
      }
    }
  ]
}
